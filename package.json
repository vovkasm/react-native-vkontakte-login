{
  "name": "@vovkasm/react-native-vkontakte-login",
  "version": "1.1.1",
  "description": "React Native wrapper for VK SDK Login",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf ./lib/ && mkdir lib && tsc",
    "prepublishOnly": "npm run build",
    "prettier": "npm run prettier:js && npm run prettier:native",
    "prettier:js": "prettier --write '**/*.{js,md,json,ts,tsx}'",
    "prettier:native": "uncrustify -c .uncrustify.cfg --no-backup ios/*.{h,m}",
    "start": "react-native start --projectRoot samples/VkLoginSample",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vovkasm/react-native-vkontakte-login.git"
  },
  "keywords": [
    "react",
    "react-native",
    "vk",
    "vkontakte",
    "sdk",
    "login",
    "share"
  ],
  "author": "Vladimir Timofeev <vovkasm@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vovkasm/react-native-vkontakte-login/issues"
  },
  "homepage": "https://github.com/vovkasm/react-native-vkontakte-login#readme",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "@types/react": "^16.8.12",
    "@types/react-native": "^0.57.42",
    "@vovkasm/tslint-config": "^2.0.1",
    "date-fns": "^1.30.1",
    "metro-react-native-babel-preset": "^0.53.1",
    "prettier": "^1.17.0",
    "react": "16.8.3",
    "react-native": "0.59.4",
    "tslint": "^5.15.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.3"
  },
  "dependencies": {
    "tslib": "^1.9.0"
  },
  "peerDependencies": {
    "react-native": ">=0.51.0"
  }
}
